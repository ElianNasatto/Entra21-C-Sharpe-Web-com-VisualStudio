
@{
    Layout = null;
}

<!DOCTYPE html>

<html>


<head>

    <meta name="viewport" content="width=device-width" />
    <title>Index</title>

</head>


<body>
    <div> 
        <a href="/estoque/cadastro">Cadastro Estoque</a>
    </div>

    <table>

        <thead>
            <tr>
                <td>Código</td>
                <td>Nome</td>
                <td>Quantidade</td>
                <td>Valor</td>
            </tr>
        </thead>

        <tbody>
            @for (int i = 0; i < @ViewBag.Estoques.Count; i++)
            {
            <tr>
                <td>@ViewBag.Estoques[i].Id</td>
                <td>@ViewBag.Estoques[i].Nome</td>
                <td>@ViewBag.Estoques[i].Quantidade</td>
                <td>@ViewBag.Estoques[i].Valor</td>

            </tr>
            }
        </tbody>


        <tfoot>
            <tr>
                <td>Código</td>
                <td>Nome</td>
                <td>Quantidade</td>
                <td>Valor</td>
            </tr>
        </tfoot>
    </table>

</body>




</html>
